<!-- Meus FIIs begin -->
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header border-bottom">
                <h6 class="m-0">Meus FIIs</h6>
            </div>
            <div class="card-body">
                <div class="row border-bottom">
                    <div class="col-12 col-sm-6 d-flex mb-2">
                        <a class="btn btn-success" href="fiis-new.html" role="button">Adicionar</a>
                    </div>

                </div>
                <table class="table table-striped">
                    <thead>
                    <tr>
                        <th scope="col">Ticket</th>
                        <th scope="col">Quant.</th>
                        <th scope="col">Valor</th>
                        <th scope="col">Total</th>
                        <th scope="col">Preço</th>
                        <th scope="col">Preço Total</th>
                        <th scope="col">Val</th>
                        <th scope="col">Rendimentos</th>
                        <th scope="col">Rendimentos total Mês</th>
                        <th scope="col" colspan="6">Rendimentos 6 meses</th>
                        <th scope="col">Ações</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#fiis.userEquities}}
                        <tr class="{{#val}}table-danger{{/val}}">
                            <td>{{symbol}}</td>
                            <td>{{qty}}</td>
                            <td>R$ {{value}}</td>
                            <td>R$ {{total}}</td>
                            <td>R$ {{price}}</td>
                            <td>R$ {{price_total}}</td>
                            <td>{{val}}%</td>
                            <td>R$ 0.9</td>
                            <td>R$ 9.00</td>
                            <td>R$ 0.9</td>
                            <td>R$ 0.9</td>
                            <td>R$ 0.9</td>
                            <td>R$ 0.9</td>
                            <td>R$ 0.9</td>
                            <td>R$ 0.9</td>
                            <td style="width: 20%">
                                <a class="btn btn-success btn-sm" href="fiis-new.html" role="button"><i class="fas fa-plus"></i>&nbsp;Comprar</a>
                                <a class="btn btn-danger btn-sm" href="fiis-sell.html" role="button"><i class="fas fa-plus"></i>&nbsp;Vender</a>
                                <a class="btn btn-primary btn-sm" href="fiis-details.html" role="button"><i class="fas fa-info"></i>&nbsp;Detalhes</a>
                            </td>
                        </tr>
                    {{/fiis.userEquities}}
                    {{^fiis.userEquities}}
                        <tr class="">
                            <td colspan="5">No data</td>
                        </tr>
                    {{/fiis.userEquities}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<!-- Meus FIIs end -->