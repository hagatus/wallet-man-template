<!doctype html>
<html class="no-js h-100" lang="pt">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <title>{{default.projectName}} - {{fiis.title}}</title>

        {{>partials/headAssets}}
    </head>
    <body class="layout-fixed">
        <div class="container-fluid">
            <div class="row">
                {{>partials/sidebar}}

                <main class="main-content">

                    {{>partials/mainNavbar}}

                    <div class="main-content-container">
                        <!-- page header begin -->
                        <div class="page-header row">
                            <div class="col-12 col-sm-4 text-center text-sm-left mb-0">
                                <span class="text-uppercase page-subtitle">{{default.dashboard}}</span>
                                <h3 class="page-title">FIIs - Visão Geral</h3>
                            </div>
                        </div>
                        <!-- page header end -->
                        <div class="row">
                            <div class="col-lg-6">
                                <!-- FIIs composição gráficos begin -->
                                <div class="card" id="fii-composition">
                                    <div class="card-header border-bottom">
                                        <h6 class="m-0">FIIs - Composição</h6>
                                    </div>
                                    <div class="card-body d-flex">
                                        <canvas height="340" style="max-width: 100% !important;"
                                                id="fii-composition-graph" class="m-auto"></canvas>
                                    </div>
                                    <div class="card-footer">
                                        <a class="btn btn-primary" href="#" role="button">Mais detalhes</a>
                                    </div>
                                </div>
                                <!-- FIIs composição gráficos end -->
                            </div>
                            <div class="col-lg-6">
                                <!-- FIIs evolução gráficos begin -->
                                <div class="card" id="fii-evolution">
                                    <div class="card-header border-bottom">
                                        <h6 class="m-0">FIIs - Evolução</h6>
                                    </div>
                                    <div class="card-body d-flex">
                                        <div class="date-range">

                                        </div>
                                        <canvas height="340" style="max-height: 300px !important;"
                                                id="fii-evolution-graph" class=""></canvas>
                                    </div>
                                    <div class="card-footer">
                                        <a class="btn btn-primary" href="#" role="button">Mais detalhes</a>
                                    </div>
                                </div>
                                <!-- FIIs evolução gráficos end -->
                            </div>
                        </div>


                        <div class="row">
                            <div class="col-lg-12">
                                <div class="card">
                                    <div class="card-header border-bottom">
                                        <h6 class="m-0">Meus FIIs</h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="row border-bottom">
                                            <div class="col-12 col-sm-6 d-flex mb-2">
                                                <a class="btn btn-primary" href="fiis-new.html" role="button">Adicionar Novo</a>
                                            </div>

                                        </div>
                                        <table class="table table-striped">
                                            <thead>
                                            <tr>
                                                <th scope="col">FII</th>
                                                <th scope="col">Quant.</th>
                                                <th scope="col">Valor</th>
                                                <th scope="col">Total</th>
                                                <th scope="col">Preço</th>
                                                <th scope="col">Preço Total</th>
                                                <th scope="col">Val</th>
                                                <th scope="col">Rendimentos</th>
                                                <th scope="col">Rendimentos total Mês</th>
                                                <th scope="col" colspan="6">Rendimentos 6 meses</th>
                                                <th scope="col">Ações</th>
                                            </tr>
                                            </thead>
                                            <tbody>
                                                {{#fiis.list}}
                                                <tr class="{{#val}}table-danger{{/val}}">
                                                    <td>{{symbol}}</td>
                                                    <td>{{qty}}</td>
                                                    <td>R$ {{value}}</td>
                                                    <td>R$ {{total}}</td>
                                                    <td>R$ {{price}}</td>
                                                    <td>R$ {{price_total}}</td>
                                                    <td>{{val}}%</td>
                                                    <td>R$ 0.9</td>
                                                    <td>R$ 9.00</td>
                                                    <td>R$ 0.9</td>
                                                    <td>R$ 0.9</td>
                                                    <td>R$ 0.9</td>
                                                    <td>R$ 0.9</td>
                                                    <td>R$ 0.9</td>
                                                    <td>R$ 0.9</td>
                                                    <td>
                                                        <a class="btn btn-primary" href="fiis-details.html" role="button">Mais detalhes</a>
                                                    </td>
                                                </tr>
                                                {{/fiis.list}}
                                                {{^fiis.list}}
                                                    <tr class="">
                                                        <td colspan="5">No data</td>
                                                    </tr>
                                                {{/fiis.list}}
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                </main>
                {{>partials/footer}}
            </div>
        </div>
        <!-- last scripts -->
        <script type="text/javascript" src="js/fiis.1.0.0.js"></script>
        <!-- last scripts -->
    </body>
</html>